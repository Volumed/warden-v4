{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		"moduleResolution": "nodeNext",
		"outDir": "dist",
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"strict": true,
		"incremental": true,
		"resolveJsonModule": true
	}
}
